      "fragment": "uniform float time; varying vec2 vUv; varying vec3 vWorldPos; float hash(vec2 p) { return fract(sin(dot(p, vec2(127.1, 311.7))) * 43758.5453); } float noise(vec2 p) { vec2 i = floor(p); vec2 f = fract(p); f = f * f * (3.0 - 2.0 * f); float a = hash(i); float b = hash(i + vec2(1.0, 0.0)); float c = hash(i + vec2(0.0, 1.0)); float d = hash(i + vec2(1.0, 1.0)); return mix(mix(a, b, f.x), mix(c, d, f.x), f.y); } void main() { vec3 waterDeep = vec3(0.0, 0.35, 0.5); vec3 waterShallow = vec3(0.2, 0.6, 0.7); vec3 highlight = vec3(0.9, 0.95, 1.0); vec2 pos = vWorldPos.xz; float wave1 = sin(pos.x * 3.0 + time * 1.5) * 0.5 + 0.5; float wave2 = sin(pos.y * 2.5 - time * 1.2) * 0.5 + 0.5; float wave3 = sin((pos.x + pos.y) * 2.0 + time * 0.8) * 0.5 + 0.5; float waves = (wave1 + wave2 + wave3) / 3.0; float n = noise(pos * 4.0 + time * 0.3) * 0.3; float ripple = noise(pos * 8.0 - time * 0.5) * 0.15; float pattern = waves * 0.4 + n + ripple; vec3 color = mix(waterDeep, waterShallow, pattern); float sparkle = pow(max(0.0, sin(pos.x * 20.0 + time * 3.0) * sin(pos.y * 20.0 - time * 2.5)), 8.0) * 0.5; color = mix(color, highlight, sparkle); float edge = 1.0 - smoothstep(0.7, 1.0, length(vUv - 0.5) * 2.0); color = mix(waterDeep * 0.8, color, edge); gl_FragColor = vec4(color, 0.9); }",
