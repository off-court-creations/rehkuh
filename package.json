{
  "name": "rehkuh",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist \"*.tsbuildinfo\"",
    "dev": "vite --host",
    "build": "npm run -s clean && tsc -b && vite build",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" && echo \"No ESLint issues found.\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix && echo \"No ESLint issues found.\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.{ts,tsx,js,jsx,json,md,css}\" --check",
    "format:fix": "prettier --ignore-path .gitignore \"src/**/*.{ts,tsx,js,jsx,json,md,css}\" --write",
    "typecheck": "tsc --noEmit -p tsconfig.app.json && tsc --noEmit -p tsconfig.node.json",
    "preview": "vite preview",
    "promote-staging": "npx tsx scripts/promote-staging.ts",
    "copy-to-staging": "npx tsx scripts/copy-to-staging.ts"
  },
  "dependencies": {
    "@archway/valet": "^0.34.0",
    "@react-three/drei": "^10.7.0",
    "@react-three/fiber": "^9.3.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.0",
    "three": "^0.179.0",
    "zod": "^4.3.4",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^4.4.1",
    "chokidar": "^5.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.37.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "typescript": "^5.8.3",
    "vite": ">=6.4.1"
  }
}
